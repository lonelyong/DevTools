<views:DefaultWindow
	x:Class="DevTools.Client.Views.Main.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:views="clr-namespace:DevTools.Client.Views"
    xmlns:views_main="clr-namespace:DevTools.Client.Views.Main"
    Title="MainWindow" Height="450" Width="800"
	StateChanged="Window_StateChanged" Initialized="DefaultWindow_Initialized" Loaded="DefaultWindow_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="109*"/>
            <ColumnDefinition Width="684*"/>
        </Grid.ColumnDefinitions>
        <TabControl Margin="10,10,10.333,9.667" Grid.ColumnSpan="2">
            <TabItem Header="RSA">
                <views_main:RsaTab/>
            </TabItem>
            <TabItem Header="DES">

            </TabItem>
            <TabItem Header="AES">

            </TabItem>
            <TabItem Header="HASH">

            </TabItem>
            <TabItem Header="SLink">
                <views_main:SLinkTab/>
            </TabItem>
        </TabControl>
    </Grid>
</views:DefaultWindow>
